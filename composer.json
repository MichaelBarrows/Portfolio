{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.2.117",
        "filament/notifications": "^3.2.117",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/framework": "^10.48.22",
        "laravel/sanctum": "^3.3.3",
        "laravel/socialite": "^5.16.0",
        "laravel/tinker": "^2.10",
        "livewire/livewire": "^3.5.6",
        "predis/predis": "^2.2.2",
        "pusher/pusher-php-server": "^7.2.4",
        "sentry/sentry-laravel": "^4.9",
        "socialiteproviders/laravelpassport": "^4.3",
        "socialiteproviders/spotify": "^4.1",
        "wire-elements/modal": "^2.0.12"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14.3",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.18.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^7.10",
        "pestphp/pest": "^2.35.1",
        "pestphp/pest-plugin-livewire": "^2.1",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pint": [
            "./vendor/bin/pint --preset=psr12"
        ],
        "pest": [
            "./vendor/bin/pest tests"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
